@{
    ViewData["Title"] = "Home Page";
}
<h1 class="document__title main__title">Sessions</h1>
<a asp-action="Count" class="btn-primary start__btn">
    Start new session
</a>
<table class="table table-striped table__sessions">
    <thead>
    <tr>
        <td>Start Date</td>
        <td>End Date</td>
        <td>Actions</td>
    </tr>
    </thead>
    <tbody>
        @foreach (Session s in Model.Sessions)
        {
            <partial name="RowPartial" model="s"/>
        }
    </tbody>
</table>